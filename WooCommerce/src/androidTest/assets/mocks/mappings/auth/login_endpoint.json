{
  "request": {
    "method": "POST",
    "urlPath": "/wp-login.php?action=login-endpoint",
    "bodyPatterns": [
      {
        "matches": ".*username=[^&]+.*"
      },
      {
        "matches": ".*password=((?!invalidPswd)[^&])+(&.*|$)"
      },
      {
        "matches": ".*client_secret=.*"
      }
    ]
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "access_token": "valid_token",
      "token_type": "bearer",
      "blog_id": "0",
      "blog_url": null,
      "scope": "global"
    },
    "headers": {
      "Content-Type": "application/json",
      "Connection": "keep-alive"
    }
  }
}
